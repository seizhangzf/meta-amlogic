+    DRM_BOOL  fWasPreAlloc; /* used to signal that we PreAlloc, and Alloc shouldn't fill mem with 0xaa */
+DRM_OBFUS_FILL_BYTES(4)
+} DRM_STACK_ALLOCATOR_CONTEXT;
