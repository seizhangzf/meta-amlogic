+    __in                                  const DRM_DWORD                   f_cbResponse,
+    __in_bcount(f_cbResponse)             const DRM_BYTE                   *f_pbResponse,
+    __out_opt                                   DRM_DWORD                  *f_pcchCustomData,
+    __deref_out_ecount_opt(*f_pcchCustomData)   DRM_CHAR                  **f_ppchCustomData );
+
+/*********************************************************************
+**
