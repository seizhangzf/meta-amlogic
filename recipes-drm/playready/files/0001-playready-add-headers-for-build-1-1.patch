+#define DRM_BUILD_PROFILE_PC                          21
+#define DRM_BUILD_PROFILE_RESERVED                    22
+#define DRM_BUILD_PROFILE_WP8_1                       23
+#define DRM_BUILD_PROFILE_PK_TEST_MIN                100
+#define DRM_BUILD_PROFILE_PK_TEST_OEM_PSEUDO_TEE     133
