+    __inout_ecount(lnga + lngb)       digit_t  *c );
+
+#else  /* !DRM_INLINING_SUPPORTED */
+
+/****************************************************************************/
+static DRM_BOOL DRM_CALL multiply(
+                                const digit_t  *a,
+                                const DRM_DWORD lnga,
+                                const digit_t  *b,
+                                const DRM_DWORD lngb,
+    __inout_ecount(lnga + lngb)       digit_t  *c )
+/*
