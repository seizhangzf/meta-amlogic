+#define ENSURE_MEMBERS_ADJACENT(struc,member1,member2)  ((DRM_OFFSET_OF(struc,member1)+(DRM_DWORD)(sizeof(((struc *)0)->member1))) == DRM_OFFSET_OF(struc,member2))
