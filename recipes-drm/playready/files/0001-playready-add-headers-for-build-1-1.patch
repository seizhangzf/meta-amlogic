+#define DRM_AES_ROUNDS_192    12
+#define DRM_AES_ROUNDS_256    14
+#define DRM_AES_MAXROUNDS     DRM_AES_ROUNDS_256
+
+#define DRM_AES_BLOCKLEN                 DRM_AES_KEYSIZE_128    /* Size ( in bytes ) of a single encrypted/decrypted block */
