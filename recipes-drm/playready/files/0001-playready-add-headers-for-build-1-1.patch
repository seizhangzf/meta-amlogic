+#define DRM_BUILD_PROFILE_IOS                        700
+#define DRM_BUILD_PROFILE_RMSDK                      800
+#define DRM_BUILD_PROFILE_LINUX                      900
+#define DRM_BUILD_PROFILE_MPR                       1000
