+**
+***@@@---@@@@******************************************************************
+*/
+
+#ifndef __OEMDES_H__
+#define __OEMDES_H__
+
+#include <drmtypes.h>
+
+ENTER_PK_NAMESPACE;
+
+#ifndef __DES_H__
+
+typedef struct _destable
+{
+    DRM_DWORD keytab[16][2];
+} DESTable;
+
+#define DES_TABLESIZE    sizeof(DESTable)
+#define DES_BLOCKLEN    8
+#define DES_KEYSIZE        8
+
