+DRM_API_VOID DRM_VOID* DRM_CALL bignum_alloc(__in const DRM_DWORD cblen, __in struct bigctx_t *f_pBigCtx);
+DRM_API_VOID DRM_VOID  DRM_CALL bignum_free(DRM_VOID *pvMem, struct bigctx_t *f_pBigCtx);
